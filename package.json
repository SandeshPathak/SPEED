{
  "name": "speed",
  "version": "1.0.0",
  "description": "website: https://speed-website.herokuapp.com/",
  "main": "app.js",
  "scripts": {
    "build": "cd frontend && npm run build",
    "install-client": "cd frontend && npm install",
    "heroku-postbuild": "npm run install-client && cd frontend && npm install && npm run build",
    "server": "node app.js",
    "develop": "concurrently --kill-others-on-fail \"npm run server\"\"npm run start --prefix frontend\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix frontend\"",
    "app": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
