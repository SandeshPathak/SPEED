  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "concurrently": "^7.4.0",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mongoose": "^6.6.0",
    "serve": "^14.0.1",
    "validation": "^0.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  },
